<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="https://jakarta.ee/xml/ns/jakartaee" xsi:schemaLocation="https://jakarta.ee/xml/ns/jakartaee https://jakarta.ee/xml/ns/jakartaee/web-app_6_0.xsd" id="WebApp_ID" version="6.0">
  
  <servlet>
  	<servlet-name>add</servlet-name>
  	<servlet-class>com.learnservletandjsp.AddServlet</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>add</servlet-name>
  	<url-pattern>/add</url-pattern>
  </servlet-mapping>
  
  <servlet>
  	<servlet-name>sq</servlet-name>
  	<servlet-class>com.learnservletandjsp.SquareServlet</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>sq</servlet-name>
  	<url-pattern>/square</url-pattern>
  </servlet-mapping>
  
  <servlet>
  	<servlet-name>cube</servlet-name>
  	<servlet-class>com.learnservletandjsp.CubeServlet</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>cube</servlet-name>
  	<url-pattern>/cube</url-pattern>
  </servlet-mapping>
  
  
  <servlet>
  	<servlet-name>configcontext</servlet-name>
  	<servlet-class>com.learnservletandjsp.ConfigContext</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>configcontext</servlet-name>
  	<url-pattern>/config</url-pattern>
  </servlet-mapping>
 	
  <context-param>
  	<param-name>name</param-name>
  	<param-value>Batman</param-value>
  </context-param>
	 
</web-app>